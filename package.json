{
  "name": "fdb",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.21.1",
  "devEngines": {
    "packageManager": {
      "name": "bun",
      "version": "1.21.1"
    },
    "runtime": {
      "name": "bun",
      "version": "1.21.1"
    }
  },
  "scripts": {
    "dev": "bun run ./src/index.ts",
    "generate": "bunx drizzle-kit generate --dialect=sqlite --schema='./src/schema.ts'",
    "migrate": "bun run generate && bun run scripts/migrate.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.7"
  }
}