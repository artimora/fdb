{
    "name": "@copperdevs/fdb",
    "version": "1.0.0",
    "type": "module",
    "dependencies": {
        "kysely": "^0.28.8"
    },
    "devDependencies": {
        "@types/bun": "^1.2.12"
    },
    "scripts": {
        "build": "rm -rf dist && bun run --bun build.ts"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "bun": "./dist/bun/index.js"
        },
        "./errors": {
            "types": "./dist/errors.d.ts",
            "import": "./dist/errors.js",
            "require": "./dist/errors.cjs"
        },
        "./main": {
            "types": "./dist/main.d.ts",
            "import": "./dist/main.js",
            "require": "./dist/main.cjs"
        },
        "./migrations": {
            "types": "./dist/migrations.d.ts",
            "import": "./dist/migrations.js",
            "require": "./dist/migrations.cjs"
        },
        "./types": {
            "types": "./dist/types.d.ts",
            "import": "./dist/types.js",
            "require": "./dist/types.cjs"
        },
        "./util": {
            "types": "./dist/util.d.ts",
            "import": "./dist/util.js",
            "require": "./dist/util.cjs"
        },
        "./operations/directory": {
            "types": "./dist/operations/directory.d.ts",
            "import": "./dist/operations/directory.js",
            "require": "./dist/operations/directory.cjs"
        },
        "./operations/file": {
            "types": "./dist/operations/file.d.ts",
            "import": "./dist/operations/file.js",
            "require": "./dist/operations/file.cjs"
        }
    }
}
